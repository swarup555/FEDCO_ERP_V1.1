    <link href="~/Content/dist/font-awesome.min.css" rel="stylesheet" />
   
    <!-- jPList start -->
    <script>
        $('document').ready(function () {

            $('#demo').jplist({
                itemsBox: '.list'
				, itemPath: '.list-item'
				, panelPath: '.jplist-panel'
            });
        });
    </script>

