                @foreach(var obj in Model)
                {
                    //int i=Model.Count;
                  
                        <div class="form-group">
                           
                            <div class="col-md-4 col-sm-6 col-xs-12 ">
                                <div class="card-container">
                                    <div class="card">
                                        <div class="front">
                                            <div class="cover">
                                                <img src="~/images/rotating_card_thumb2.png" />
                                            </div>
                                            <div class="user">
                                                @if (@obj.EMPIMAGE == null)
                                                {
                                                <img class="img-circle" src="~/images/user.png" alt="~/images/rotating_card_profile3.png" />
                                                }
                                                else{
                                                <img class="img-circle" src=@obj.EMPIMAGE alt="~/images/rotating_card_profile3.png" />
                                                }
                                               
                                            </div>
                                            <div class="content">
                                                <div class="main">
                                                    <h3 class="name">@obj.EMPLOYEE_FULLNAME</h3>
                                                    <p class="text-center"><i class="fa fa-briefcase user-profile-icon"></i> @obj.designationname</p>
                                                    <p class="text-center"><i class="fa fa-tag user-profile-icon"></i> @obj.departmentname</p>
                                                    @if (@obj.DIVISIONNAME != null &&@obj.SUB_DIVISIONNAME!=null&&@obj.SECTIONNAME!=null)
                                                    {
                                                    <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> @obj.DIVISIONNAME,@obj.SUB_DIVISIONNAME,@obj.SECTIONNAME</p>
                                                    }
                                                    else if (@obj.DIVISIONNAME != null &&@obj.SUB_DIVISIONNAME!=null)
                                                    {
                                                     <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> @obj.DIVISIONNAME,@obj.SUB_DIVISIONNAME</p>
                                                    }
                                                    else if (@obj.DIVISIONNAME != null)
                                                    {
                                                     <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> @obj.DIVISIONNAME</p>
                                                    }
                                                    else if (@obj.SUB_DIVISIONNAME!= null)
                                                    {
                                                    <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> @obj.SUB_DIVISIONNAME</p>
                                                    }
                                                    else if (@obj.SECTIONNAME!=null)
                                                    {
                                                     <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> @obj.SECTIONNAME</p>
                                                    }
                                                    else if (@obj.WORKLOCATION == Convert.ToDecimal(5))
                                                    {
                                                        <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> Khordha</p>
                                                    }
                                                    else if (@obj.WORKLOCATION == Convert.ToDecimal(7))
                                                    {
                                                         <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> Project Office</p>
                                                    }
                                                    else if (@obj.WORKLOCATION == Convert.ToDecimal(8))
                                                    {
                                                         <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> Kandhamal</p>
                                                    }
                                                    else if (@obj.WORKLOCATION == Convert.ToDecimal(9))
                                                    {
                                                         <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> Gurgaon</p>
                                                    }
                                                    else if (@obj.WORKLOCATION == Convert.ToDecimal(10))
                                                    {
                                                         <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> Mumbai</p>
                                                    }
                                                    else if (@obj.WORKLOCATION == Convert.ToDecimal(11))
                                                    {
                                                         <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> puri</p>
                                                    }
                                                    else if (@obj.WORKLOCATION == Convert.ToDecimal(1))
                                                    {
                                                         <p class="text-center"><i class="fa fa-map-marker user-profile-icon"></i> Fortune Tower , Bhubaneswar</p>
                                                    }
                                                </div>
                                                <div class="footer">
                                                    <i class="fa fa-user"></i> FEDCO-HRMS
                                                </div>
                                            </div>
                                        </div> <!-- end front panel -->
                                        <div class="back">
                                            <div class="header">
                                                <h5 class="motto">"FEDCO : BUILDING STRONG CONNECTIONS"</h5>
                                            </div>
                                            <div class="content">
                                                <div class="main">
                                                    <h4 class="text-center">Address</h4>
                                                    @if (@obj.Permanentaddress != null)
                                                    {
                                                         <p class="text-center">@obj.Permanentaddress</p>
                                                    }
                                                    else
                                                    {
                                                         <p class="text-center"> NA </p>
                                                    }
                                                    @if (@obj.MOBILE_NUMBER != null)
                                                    {
                                                        <p class="text-center">PERSONAL NUMBER : @obj.MOBILE_NUMBER</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="text-center">PERSONAL NUMBER : NA </p>
                                                    }
                                                    @if (@obj.EMERGENCY_CONTACT_NUMBER != null)
                                                    {
                                                         <p class="text-center">EMERGENCY CONTACT NUMBER : @obj.EMERGENCY_CONTACT_NUMBER</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="text-center">EMERGENCY CONTACT NUMBER : NA </p>
                                                    }
                                                    @if (@obj.OFFICIAL_MOBILE_NUMBER != null)
                                                    {
                                                         <p class="text-center">OFFICIAL MOBILE NUMBER : @obj.OFFICIAL_MOBILE_NUMBER</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="text-center">OFFICIAL MOBILE NUMBER : NA </p>
                                                    }
                                                    @if (@obj.PERSONAL_EMAIL != null)
                                                    {
                                                         <p class="text-center">PERSONAL EMAIL : @obj.PERSONAL_EMAIL</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="text-center"> PERSONAL EMAIL : NA </p>
                                                    }
                                                    @if (@obj.OFFICIAL_EMAIL != null)
                                                    {
                                                        <p class="text-center">OFFICIAL EMAIL : @obj.OFFICIAL_EMAIL</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="text-center"> OFFICIAL EMAIL : NA </p>
                                                    }
                                                    
                                                    @*<div class="stats-container">
                                                        <div class="stats">
