                                                            <h4>35</h4>
                                                            <p>
                                                                Projects
                                                            </p>
                                                        </div>
                                                    </div>*@
                                                </div>
                                            </div>
                                            <div class="footer">
                                               
                                                    <a href="@Url.Action("Index/" + Convert.ToInt32(obj.ID), "EmployeeDetails")" class="google"><i class="fa fa-edit"></i>CLICK HERE FOR DETAILS</a>
                                               
                                            </div>
                                        </div> <!-- end back panel -->
                                    </div> <!-- end card -->
                                </div>
                            </div>
                           
                        </div>
                }
